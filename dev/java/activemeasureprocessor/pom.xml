					<archive>
						<manifestEntries>
							<mode>development</mode>
							<Version>${VERSION_NUMBER}</Version>
						</manifestEntries>
					</archive>
				</configuration>
			</plugin>
        </plugins>
    </build>
    <profiles>
       <profile>
           <id>useJenkinsDefaultFile</id>
           <activation>
               <activeByDefault>false</activeByDefault>
           </activation>
           <build>
               <plugins>
                   <plugin>
                       <groupId>org.apache.maven.plugins</groupId>
                       <artifactId>maven-antrun-plugin</artifactId>
                       <version>${maven-antrun-plugin.version}</version>
                       <executions>
                           <execution>
                               <id>copyJenkinsDefaultFile</id>
                               <phase>generate-test-resources</phase>
                               <configuration>
                                   <target>
                                       <echo message="Using the default jenkins file to replace ${project.basedir}/src/test/resources/dev-eval-functions-test-data.xlsx" />
                                       <copy file="../../../jenkinsParameterFile.txt" toFile="${project.basedir}/src/test/resources/dev-eval-functions-test-data.xlsx" overwrite="true" />
                                       <echo message="Using the default jenkins file to replace ${project.basedir}/src/test/resources/dev_java_model_test_data.xlsx" />
                                       <copy file="../../../jenkinsParameterFile.txt" toFile="${project.basedir}/src/test/resources/dev_java_model_test_data.xlsx" overwrite="true" />
                                       <echo message="Using the default jenkins file to replace ${project.basedir}/src/test/java/net/ahm/careengine/bomext/clinical/Test_Case_C32Expressions.xls" />
                                       <copy file="../../../jenkinsParameterFile.txt" toFile="${project.basedir}/src/test/java/net/ahm/careengine/bomext/clinical/Test_Case_C32Expressions.xls" overwrite="true" />                                       
                                   </target>
                               </configuration>
                               <goals>
                                   <goal>run</goal>
                               </goals>
                           </execution>
                       </executions>
                   </plugin>
               </plugins>
           </build>
       </profile>
