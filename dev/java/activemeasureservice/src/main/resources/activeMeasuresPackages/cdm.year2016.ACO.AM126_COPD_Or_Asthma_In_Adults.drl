rule "AM126_NUM_DX_7931_8833"
	dialect "mvel"
	when
		 $flm : FactLevelMeasureDenominator( measureId == 126 , $primaryFact : primaryOriginationFact != null )
		 $readmission : ClaimHeader( this != $primaryFact , claimType == ClaimType.INPAT  || == ClaimType.INPAT_FULLENC , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate , principalDiagnosisElements contains 7931 , allDiagnosticEventElements contains 8833 )
	then
		FactLevelMeasureNumerator flmNumerator = $flm.createFactLevelMeasureNumeratorWithPrimaryFact($readmission); insert(flmNumerator);
end


rule "AM126_NUM_EXCL_CYSTIC_FIBROSIS_RESPIRATORY_ANOMALIES"
	dialect "mvel"
	when
		$flmNum : FactLevelMeasureNumerator( measureId == 126 , $primaryFact : primaryFact != null )
		ClaimHeader( allDiagnosticEventElements contains 8600 ) from $primaryFact
	then
		$flmNum.setExcludedFromNumerator( true );
end


rule "AM126_DENOM"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 40 , gender != null )
		 $claimHdr : ClaimHeader( endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate , claimType == ClaimType.INPAT  || == ClaimType.OUTPAT  || == ClaimType.INPAT_FULLENC , allDiagnosticEventElements contains 7931  || contains 8834 )
	then
		FactLevelMeasureDenominator flmDenominator= factLevelMeasureBuilder.newFactLevelMeasureDenominatorWithPrimaryFact(126, $claimHdr); insert(flmDenominator)
end


