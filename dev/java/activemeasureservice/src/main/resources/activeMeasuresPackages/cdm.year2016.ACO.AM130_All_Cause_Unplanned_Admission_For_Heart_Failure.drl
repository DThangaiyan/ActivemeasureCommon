rule "AM130_NUM_EXCL_CARDIAC_PROC"
	dialect "mvel"
	when
		$flmNum : FactLevelMeasureNumerator( measureId == 130 , $primaryOrigFact : primaryFact != null )
		ClaimHeader( relatedProcedureEventElements contains 7933 ) from $primaryOrigFact
	then
		$flmNum.setExcludedFromNumerator( true );
end


rule "AM130_NUM_EXCL_READMISSION_SAME_DAY_DIFF_FACILITY"
	dialect "mvel"
	when
		 $flmNum : FactLevelMeasureNumerator( measureId == 130 , $primaryOrigFact : primaryFact != null , factLevelDenominator != null , $primaryOrigFactFromDenom : factLevelDenominator.primaryOriginationFact != null )
		 $indexEvent : ClaimHeader( $indexAdmissionAdmittingDate : startDate != null , $indexAdmissionFacility : servicingOrgId != null ) from $primaryOrigFactFromDenom
		$1dayAfterIndexAdmission : java.util.Date() from GlobalFunctions.getLaterDate($indexAdmissionAdmittingDate, 1, DateTimeUnit.DAY)
		 $readmissionEvent : ClaimHeader( startDate < ( $1dayAfterIndexAdmission ) , startDate >= $indexAdmissionAdmittingDate , servicingOrgId != $indexAdmissionFacility ) from $primaryOrigFact
	then
		 $flmNum.setExcludedFromNumerator( true );
end


rule "AM130_DENOM_HEART_FAILURE_1_INPAT_ANY_DIAG"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 65 , gender != null )
		 $primaryFact : ClaimHeader( endDate < months12BeforeMeasurementEndDate , endDate >= months24BeforeMeasurementEndDate , allDiagnosticEventElements contains 7932 , claimType == ClaimType.INPAT  || == ClaimType.INPAT_FULLENC )
	then
		FactLevelMeasureDenominator flmDenominator= factLevelMeasureBuilder.newFactLevelMeasureDenominatorWithPrimaryFact(130, $primaryFact); insert(flmDenominator)
end


rule "AM130_DENOM_HEART_FAILURE_2_OUTPAT_ANY_DIAG"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 65 , gender != null )
		 $outPatClaimHeaderEvents : java.util.Collection( size >= 2 ) from collect ( ClaimHeader( endDate < months12BeforeMeasurementEndDate , endDate >= months24BeforeMeasurementEndDate , claimType == ClaimType.OUTPAT , allDiagnosticEventElements contains 7932 )) 
	then
		FactLevelMeasureDenominator flmDenominator= factLevelMeasureBuilder.newFactLevelMeasureDenominator(130); insert(flmDenominator)
end


rule "AM130_DENOM_EXCL_PROC_8853"
	dialect "mvel"
	when
		$flmDenom : FactLevelMeasureDenominator( measureId == 130 )
		ProcedureEvent( endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate , elements contains 8853 )
	then
		$flmDenom.setExcludedFromDenominator( true );
		update( $flmDenom );
end




rule "AM130_NUM_READMISSION"
	dialect "mvel"
	when
		 $flmd : FactLevelMeasureDenominator( measureId == 130 )
		 $readmission : ClaimHeader( startDate >= months12BeforeMeasurementEndDate , startDate <= measurementEndDate , allDiagnosticEventElements contains 7940 , claimType == ClaimType.INPAT  || == ClaimType.INPAT_FULLENC )
	then
		FactLevelMeasureNumerator flmNumerator = $flmd.createFactLevelMeasureNumeratorWithPrimaryFact($readmission); insert(flmNumerator);
end


rule "AM130_NUM_EXCL_SOURCE_FROM_HOP_SNF_OTHER"
	dialect "mvel"
	when
		$flmNum : FactLevelMeasureNumerator( measureId == 130 , $primaryOrigFact : primaryFact != null )
		ClaimHeader( admissionSourceCode in ( "4", "5", "6" ) ) from $primaryOrigFact
	then
		$flmNum.setExcludedFromNumerator( true );
end


