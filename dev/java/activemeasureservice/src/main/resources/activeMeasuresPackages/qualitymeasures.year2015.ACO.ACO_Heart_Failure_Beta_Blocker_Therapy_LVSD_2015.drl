rule "DENOM_ACO_HEART_FAILURE_BETA_BLOCKER_THERAPHY_FOR_LVSD_LVEF_DIAG"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( gender != null , ageAtMeasurementEndDate >= 18 )
		 DiagnosticEvent( elements contains 6273 , endDate <= measurementEndDate )
		 ProcedureEvent( elements contains 6110 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate )
		 DiagnosticEvent( elements contains 8707 , endDate <= measurementEndDate )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(23); insert(mam)
end


