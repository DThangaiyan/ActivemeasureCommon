rule "NUM_ACO_16_QM_BMI_PAST_6_MONTHS_PROC"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 25 )
		ProcedureEvent( endDate >= months6BeforeMeasurementEndDate , endDate <= measurementEndDate , elements contains 8312 )
	then
		$am.setInNumerator( true );
end


rule "NUM_ACO_16_LOINC_PAST_6_MONTHS_LAB"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 25 )
		ActiveMeasuresMemberInfo( gender != null , ageAtMeasurementEndDate >= 18  && <= 64 )
		LabResultEvent( endDate >= months6BeforeMeasurementEndDate , endDate <= measurementEndDate , elements contains 6205 , resultValue >= 18  && <= 25 )
	then
		$am.setInNumerator( true );
end


rule "NUM_ACO_16_LOINC_MU_BMI_MGT_PAST_6_MONTHS_LAB_PROC"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 25 )
		ActiveMeasuresMemberInfo( gender != null , ageAtMeasurementEndDate >= 18  && <= 64 )
		LabResultEvent( endDate >= months6BeforeMeasurementEndDate , endDate <= measurementEndDate , elements contains 6205 , resultValue > 25  || < 18.5 )
		ProcedureEvent( endDate >= months6BeforeMeasurementEndDate , endDate <= measurementEndDate , elements contains 6206 )
	then
		$am.setInNumerator( true );
end


