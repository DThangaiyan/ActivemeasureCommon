rule "DENOM_ACO_16_QM_ENCOUNTER_ADULT_PROC"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( gender != null , ageAtMeasurementEndDate >= 18 )
		 ProcedureEvent( endDate >= months6BeforeMeasurementEndDate , endDate <= measurementEndDate , elements contains 6204 )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(25); insert(mam)
end


rule "DENOM_EXCL_ACO_16_PREGNANCY_DIAG"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 25 )
		 ActiveMeasuresMemberInfo( gender == Gender.FEMALE , ageAtMeasurementEndDate >= 18 )
		 DiagnosticEvent( endDate >= months12BeforeMeasurementEndDate , endDate <= measurementEndDate , elements contains 8019 )
	then
		$am.setExcludedFromDenominator(true)
end


