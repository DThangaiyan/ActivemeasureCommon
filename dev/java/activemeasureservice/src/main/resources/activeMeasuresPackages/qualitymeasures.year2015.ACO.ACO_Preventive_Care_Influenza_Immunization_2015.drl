global java.util.Date months24BeforeMeasurementEndDate
global java.util.Date months27BeforeMeasurementEndDate
global java.util.Date years10BeforeMeasurementEndDate
global java.util.Date years5BeforeMeasurementEndDate

























rule "DENOM_EXCL_PQRS_VACCINE_CONTRAINDICATIONS"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 20)
		 DiagnosticEvent( endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate , elements contains 4131 )
	then
		$am.setExcludedFromDenominator(true)
end


rule "DENOM_EXCL_ACO_INFLUENZA_VACCINATION_NOT_DONE"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 20)
		 ProcedureEvent( endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate , elements contains 7797 )
	then
		$am.setExcludedFromDenominator(true)
end


rule "NUM_DIAG_VACCINE_INFLUENZA"
	dialect "mvel"
	when
		$am : ActiveMeasure( $fluStartDate : alternateStartDate != null  , measureId == 20 , $fluEndDate : alternateEndDate != null )
		DiagnosticEvent( endDate <= $fluEndDate , endDate >= $fluStartDate , elements contains 2326 )
	then
		$am.setInNumerator( true );
end


rule "NUM_PROC_VACCINE_INFLUENZA"
	dialect "mvel"
	when
		$am : ActiveMeasure( $fluStartDate : alternateStartDate != null  , measureId == 20 , $fluEndDate : alternateEndDate != null )
		ProcedureEvent( endDate <= $fluEndDate , endDate >= $fluStartDate , elements contains 2105 )
	then
		$am.setInNumerator( true );
end


rule "NUM_DRUG_VACCINE_INFLUENZA"
	dialect "mvel"
	when
		$am : ActiveMeasure( $fluStartDate : alternateStartDate != null  , measureId == 20 , $fluEndDate : alternateEndDate != null )
		DrugEvent( elements contains 2755 , endDate >= $fluStartDate , endDate <= $fluEndDate )
	then
		$am.setInNumerator( true );
end


rule "DENOM_QM_PREVENTIVE_VISIT"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( $dateOfBirth : birthDate != null , gender != null )
		$dob6Mo : java.util.Date() from GlobalFunctions.getLaterDate($dateOfBirth, 6, DateTimeUnit.MONTH)
		$fluStartDate : java.util.Date() from CEDateUtil.getEalierDateForYear(measurementEndDate, 1, 1, Month.OCTOBER)
		$fluEndDate : java.util.Date() from CEDateUtil.getEalierDateForYear(measurementEndDate, 0, 31, Month.MARCH)
		$NumFluStartDate : java.util.Date() from CEDateUtil.getEalierDateForYear(measurementEndDate, 1, 1, Month.AUGUST)
		 ProcedureEvent( elements contains 8787 , endDate >= ( $dob6Mo ) , endDate >= ( $fluStartDate ) , endDate <= ( $fluEndDate ) )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasureWithAlternateStartAndEndDate(20 , $NumFluStartDate , $fluEndDate); insert(mam)
end


rule "DENOM_QM_ENCOUNTER_VISIT"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( $dateOfBirth : birthDate != null , gender != null )
		$dob6Mo : java.util.Date() from GlobalFunctions.getLaterDate($dateOfBirth, 6, DateTimeUnit.MONTH)
		$fluStartDate : java.util.Date() from CEDateUtil.getEalierDateForYear(measurementEndDate, 1, 1, Month.OCTOBER)
		$fluEndDate : java.util.Date() from CEDateUtil.getEalierDateForYear(measurementEndDate, 0, 31, Month.MARCH)
		$NumFluStartDate : java.util.Date() from CEDateUtil.getEalierDateForYear(measurementEndDate, 1, 1, Month.AUGUST)
		 java.util.Collection( size >= 2 ) from collect ( ProcedureEvent( elements contains 6204 , endDate >= ( $dob6Mo ) , endDate >= ( $fluStartDate ) , endDate <= ( $fluEndDate ) )) 
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasureWithAlternateStartAndEndDate(20 , $NumFluStartDate , $fluEndDate); insert(mam)
end






