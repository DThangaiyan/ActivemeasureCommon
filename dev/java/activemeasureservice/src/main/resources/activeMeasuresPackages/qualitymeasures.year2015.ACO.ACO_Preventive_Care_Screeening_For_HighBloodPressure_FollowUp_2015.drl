rule "DENOM_ACO_BP_SCREENING_VISIT"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( gender != null , ageAtMeasurementEndDate >= 18 )
		 ProcedureEvent( endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate , elements contains 7788 )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(24); insert(mam)
end


