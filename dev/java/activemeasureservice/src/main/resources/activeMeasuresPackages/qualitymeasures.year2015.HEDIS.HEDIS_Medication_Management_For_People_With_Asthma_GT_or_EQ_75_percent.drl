rule "DENOM_ASTHMA_MEDICATION"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 5 , ageAtMeasurementEndDate <= 64 )
		 java.util.Collection( empty == false ) from collect ( Classifier( id in ( 49, 38, 36, 51, 37 ) )) 
		 java.util.Collection( empty == false ) from collect ( Classifier( id in ( 50, 41, 39, 52, 40 ) )) 
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(56); insert(mam)
end


rule "DENOM_EXCL_ASTHMA_MEDICATION"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 56)
		 DiagnosticEvent( elements contains 6011 , endDate <= measurementEndDate )
	then
		$am.setExcludedFromDenominator(true)
end


rule "DENOM_EXCL_ASTHMA_CONTROLLER_MEDS"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 56)
		 not (DrugEvent( elements contains 8971 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
	then
		$am.setExcludedFromDenominator(true)
end


rule "NUM_PDC_GT_EQ_75"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 56 )
		 $earliestDrugEvent : DrugEvent( elements contains 8971 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )
		 not (DrugEvent( this != $earliestDrugEvent , elements contains 8971 , endDate <= $earliestDrugEvent.endDate , endDate >= months12BeforeMeasurementEndDate )) 
		 $drugCollection : java.util.Collection( this != null , empty == false ) from collect ( DrugEvent( elements contains 8971 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		$oneDayPriorToED : java.util.Date() from GlobalFunctions.startDayOfRangeEndingOn($earliestDrugEvent.endDate, 2, DateTimeUnit.DAY)
		java.lang.Number(intValue >=  75) from (GlobalFunctions.calculatePdcForTreatmentPeriod($drugCollection,$oneDayPriorToED,measurementEndDate))
	then
		 $am.setInNumerator( true );
end




