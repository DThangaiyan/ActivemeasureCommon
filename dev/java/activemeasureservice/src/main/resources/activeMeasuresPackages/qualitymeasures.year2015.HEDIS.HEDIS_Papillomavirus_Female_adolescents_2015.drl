rule "DENOM_PAPILLOMAVIRUS_VACCINE_FEMALE_AGE_9_13"
	dialect "mvel"
	when
		 $memberInfo : ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 9  && <= 13 , gender == Gender.FEMALE )
	then
		ActiveMeasure mam = (activeMeasureBuilder.newActiveMeasureWithAlternateStartDate(41 , $memberInfo.birthDate)); insert(mam)
end




rule "DENOM_EXCL_HEDIS_VACCINE_ANAPHYLAXIS"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 41 )
		DiagnosticEvent( elements contains 6082 , endDate <= measurementEndDate )
	then
		$am.setExcludedFromDenominator( true );
end


rule "NUM_VACCINE_HPV"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 41 )
		$year9AfterDOB : java.util.Date() from GlobalFunctions.getLaterDate($am.alternateStartDate, 9, DateTimeUnit.YEAR)
		$year13AfterDOB : java.util.Date() from GlobalFunctions.getLaterDate($am.alternateStartDate, 13, DateTimeUnit.YEAR)
		 $eventCollection : java.util.Collection( ) from collect ( ProcedureEvent( elements contains 4499 , endDate <= ( $year13AfterDOB ) , endDate >= ( $year9AfterDOB ) )) 
		java.util.Collection(size >=  3) from collect (Event() from(GlobalFunctions.removeTooCloseEvents($eventCollection, 1, DateTimeUnit.DAY)))
	then
		 $am.setInNumerator( true );
end




rule "DENOM_EXCL_PDD"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 41)
		 PatientDerivedEvent( elements contains 8890 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )
	then
		$am.setExcludedFromDenominator(true)
end


rule "NUM_PDD"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 41)
		 PatientDerivedEvent( endDate <= measurementEndDate , elements contains 8891 , endDate >= months12BeforeMeasurementEndDate )
	then
		$am.setInNumerator(true)
end


rule "INELIGIBLE_GAP_46_DAYS_OR_MORE_IN_YR_BEFORE_13TH_BD"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 41 )
		$year13AfterDob : java.util.Date() from GlobalFunctions.getLaterDate($am.alternateStartDate, 13, DateTimeUnit.YEAR)
		MemberInfo ( $initialGapDays2 : enrolmentGapDays ); java.util.Collection( size >=  1 ) from collect ( ContiguousDays( durationInDays >=  46) from GlobalFunctions.filterWholeDays( $initialGapDays2, GlobalFunctions.startDayOfRangeEndingOn( $year13AfterDob, 12, DateTimeUnit.MONTH ) , $year13AfterDob ) )
	then
		 $am.setEligible( false );
end


rule "ILELIGIBLE_MULTIPLE_GAPS_IN_12MO_BEFORE_13TH_BIRTHDAY"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 41 )
		$year13AfterDob : java.util.Date() from GlobalFunctions.getLaterDate($am.alternateStartDate, 13, DateTimeUnit.YEAR)
		MemberInfo ( $initialGapDays2 : enrolmentGapDays ); java.util.Collection( size >=  2 ) from collect ( ContiguousDays( durationInDays >=  1) from GlobalFunctions.filterWholeDays( $initialGapDays2, GlobalFunctions.startDayOfRangeEndingOn( $year13AfterDob, 12, DateTimeUnit.MONTH ) , $year13AfterDob ) )
	then
		 $am.setEligible( false );
end


