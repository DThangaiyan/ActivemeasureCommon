rule "AM133_DENOM_PROC_6110_DX_9052"
	dialect "mvel"
	when
		$beginingOfMSyear : java.util.Date() from GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 12, DateTimeUnit.MONTH)
		$yr18 : java.util.Date() from GlobalFunctions.getEarlierDate($beginingOfMSyear, 18, DateTimeUnit.YEAR)
		 ActiveMeasuresMemberInfo( gender != null , ageAtMeasurementEndDate <= 85 , $dob : birthDate <= ( $yr18 ) )
		$yr18AfterDob : java.util.Date() from GlobalFunctions.getLaterDate($dob, 18, DateTimeUnit.YEAR)
		 $primaryEvents : java.util.Collection( empty == false ) from collect ( ProcedureEvent( endDate >= months12BeforeMeasurementEndDate  && <= months6BeforeMeasurementEndDate , elements contains 6110 , endDate >= ( $yr18AfterDob ) )) 
		 $secondaryEvents : java.util.Collection( empty == false ) from collect ( DiagnosticEvent( elements contains 9052 , endDate >= months12BeforeMeasurementEndDate  && <= months6BeforeMeasurementEndDate , endDate >= ( $yr18AfterDob ) )) 
net.ahm.careengine.domain.measures.active.ActiveMeasureRuleContext(); 		overlapname : java.util.Collection(size >=  1) from collect (OverlappedEvents() from(GlobalFunctions.getOverlappedEvents($primaryEvents, 0, DateTimeUnit.DAY, true, 1,DateTimeUnit.DAY, GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 1, DateTimeUnit.YEAR), measurementEndDate, $secondaryEvents)))
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(133); insert(mam)
end


