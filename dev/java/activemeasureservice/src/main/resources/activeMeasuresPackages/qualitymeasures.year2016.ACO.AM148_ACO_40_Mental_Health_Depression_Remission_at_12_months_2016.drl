rule "AM148_DENOM"
	dialect "mvel"
	when
		 $labEvents : java.util.Collection( empty == false ) from collect ( LabResultEvent( elements contains 8824 , resultValue > 9 , endDate < months13BeforeMeasurementEndDate  && >= months25BeforeMeasurementEndDate )) 
		 $dxEvent : java.util.Collection( empty == false ) from collect ( DiagnosticEvent( endDate >= months25BeforeMeasurementEndDate  && < months13BeforeMeasurementEndDate , elements contains 8822  || contains 8823 )) 
net.ahm.careengine.domain.measures.active.ActiveMeasureRuleContext(); 		$earliestOverlap : OverlappedEvents(this != null) from(GlobalFunctions.getEarliestOverlappedEvent($labEvents, 0, DateTimeUnit.DAY, true, 1,DateTimeUnit.DAY, GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 25, DateTimeUnit.MONTH), measurementEndDate, $dxEvent))
		$indexdateMinus18Yr : java.util.Date() from GlobalFunctions.getEarlierDate($earliestOverlap.getContiguousDays().getStart(), 18, DateTimeUnit.YEAR)
		 ActiveMeasuresMemberInfo( $dob : birthDate != null  && <= ( $indexdateMinus18Yr ) , gender != null )
		 java.util.Collection( empty == false ) from collect ( ProcedureEvent( elements contains 6204 , endDate >= months25BeforeMeasurementEndDate  && < months13BeforeMeasurementEndDate )) 
		$altStartDate : java.util.Date() from GlobalFunctions.getLaterDate($earliestOverlap.getContiguousDays().getStart(), 11, DateTimeUnit.MONTH)
		$altEndDate : java.util.Date() from GlobalFunctions.getLaterDate($earliestOverlap.getContiguousDays().getStart(), 13, DateTimeUnit.MONTH)
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasureWithAlternateStartAndEndDate(148 , $altStartDate , $altEndDate); insert(mam)
end


rule "AM148_DENOM_EXCL_8826"
