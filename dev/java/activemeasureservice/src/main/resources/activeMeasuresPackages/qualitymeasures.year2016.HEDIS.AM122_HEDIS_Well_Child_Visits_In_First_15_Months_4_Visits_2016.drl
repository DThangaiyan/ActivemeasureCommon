rule "AM122_DENOM_4_VISITS"
	dialect "mvel"
	when
		$measurementStartDate : java.util.Date() from GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 12, DateTimeUnit.MONTH)
		$month12BeforeMSstartDate : java.util.Date() from GlobalFunctions.getEarlierDate($measurementStartDate, 1, DateTimeUnit.YEAR)
		$month15BeforeMSstartDate : java.util.Date() from GlobalFunctions.getEarlierDate($month12BeforeMSstartDate, 90, DateTimeUnit.DAY)
		$month12BeforeMSendDate : java.util.Date() from GlobalFunctions.getEarlierDate(measurementEndDate, 1, DateTimeUnit.YEAR)
		$month15BeforeMSendDate : java.util.Date() from GlobalFunctions.getEarlierDate($month12BeforeMSendDate, 90, DateTimeUnit.DAY)
		 ActiveMeasuresMemberInfo( gender != null , $dob : birthDate >= ( $month15BeforeMSstartDate ) , birthDate <= ( $month15BeforeMSendDate ) )
		$year1AfterDob : java.util.Date() from GlobalFunctions.getLaterDate($dob, 1, DateTimeUnit.YEAR)
		$month15AfterDob : java.util.Date() from GlobalFunctions.getLaterDate($year1AfterDob, 90, DateTimeUnit.DAY)
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasureWithAlternateEndDate(122 , $month15AfterDob); insert(mam)
end


rule "AM122_NUM_DIAG_6251_4_VISITS"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 122 )
		 $collection : java.util.Collection( size == 4 ) from collect ( DiagnosticEvent( elements contains 6251 , startDate <= ( $am.getAlternateEndDate() )  && <= measurementEndDate , careProviderSpecialtyGroupCode == "SPECIALTYGRP_PCP" )) 
		java.util.Collection(size ==  4) from collect (Event() from(GlobalFunctions.removeTooCloseEvents($collection, 1, DateTimeUnit.DAY)))
	then
		$am.setInNumerator(true)
end


rule "AM122_NUM_PROC_3267_4_VISITS"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 122 )
		 $collection : java.util.Collection( size == 4 ) from collect ( ProcedureEvent( elements contains 3267 , careProviderSpecialtyGroupCode == "SPECIALTYGRP_PCP" , startDate <= ( $am.getAlternateEndDate() )  && <= measurementEndDate )) 
		java.util.Collection(size ==  4) from collect (Event() from(GlobalFunctions.removeTooCloseEvents($collection, 1, DateTimeUnit.DAY)))
	then
		$am.setInNumerator(true)
end


