rule "AM68_DENOM_AMI_ACUTE_DISCHARGE_DIRECT_TRANSFER"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18 , gender != null )
		$july1YearPrior : java.util.Date() from GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 18, DateTimeUnit.MONTH)
		$june30YearCurrent : java.util.Date() from GlobalFunctions.getEarlierDate(measurementEndDate, 6, DateTimeUnit.MONTH)
		 $firstInPatDischargeEvent : BaseAdmissionDischardTransferEvent( $firstEventDischargeDate : rawEndDate != null , rawEndDate <= ( $june30YearCurrent ) , rawEndDate >= ( $july1YearPrior ) , allDiagnosticEventElements contains 406 , relatedProcedureEventElements contains 8927  && excludes 8928 )
		 not (BaseAdmissionDischardTransferEvent( rawEndDate != null  && <= ( $june30YearCurrent ) , rawEndDate < $firstEventDischargeDate  && >= ( $july1YearPrior ) , allDiagnosticEventElements contains 406 )) 
		$1dayAfterFirstEventDischargeDate : java.util.Date() from GlobalFunctions.getLaterDate($firstEventDischargeDate, 1, DateTimeUnit.DAY)
		 BaseAdmissionDischardTransferEvent( this != $firstInPatDischargeEvent , startDate >= $firstEventDischargeDate , startDate <= ( $1dayAfterFirstEventDischargeDate ) , relatedProcedureEventElements contains 8927  && excludes 8928 , $secondEventDischargeDate : rawEndDate != null , rawEndDate <= ( $june30YearCurrent ) , rawEndDate >= ( $july1YearPrior ) )
	then
		ActiveMeasure mam = (activeMeasureBuilder.newActiveMeasureWithAlternateStartDate(68 , $secondEventDischargeDate)); insert(mam)
end


