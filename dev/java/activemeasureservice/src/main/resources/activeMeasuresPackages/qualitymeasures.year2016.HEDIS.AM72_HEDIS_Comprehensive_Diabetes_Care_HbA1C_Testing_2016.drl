rule "AM72_DENOM_HBA1C_TESTING"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18  && <= 75 )
		 Classifier( id in ( 27, 25, 26 ) )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(72); insert(mam)
end


rule "AM72_DENOM_EXCL_HEDIS_DIABETES_STEROID_INDUCED"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 72 )
		DiagnosticEvent( elements contains 5877 , endDate <= measurementEndDate , endDate >= months24BeforeMeasurementEndDate )
		not (Classifier( id == 53 )) 
	then
		$am.setExcludedFromDenominator( true );
end


rule "AM72_DENOM_EXCL_HEDIS_DIABETES_GESTATIONAL_DM"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 72 )
		DiagnosticEvent( elements contains 5879 , endDate <= measurementEndDate , endDate >= months24BeforeMeasurementEndDate )
		not (Classifier( id == 53 )) 
		ActiveMeasuresMemberInfo( gender == Gender.FEMALE )
	then
		$am.setExcludedFromDenominator( true );
end


rule "AM72_DENOM_EXCL_HEDIS_DIABETES_PDD_8370_OR_8956"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 72 )
		PatientDerivedEvent( elements contains 8956  || contains 8370 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )
	then
		$am.setExcludedFromDenominator( true );
end


rule "AM72_NUM_DIABETES_CARE_HBA1C_TESTING"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 72 )
		ProcedureEvent( elements contains 5886 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )
	then
		$am.setInNumerator( true );
end


rule "AM72_NUM_LOINC_DIABETES_CARE_HBA1C_TESTING"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 72 )
		LabEvent( elements contains 5887 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )
	then
		$am.setInNumerator( true );
end




