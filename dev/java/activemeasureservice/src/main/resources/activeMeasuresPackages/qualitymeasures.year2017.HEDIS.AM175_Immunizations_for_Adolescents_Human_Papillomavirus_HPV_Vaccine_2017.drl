rule "AM175_DENOM"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 13  && < 14 , $Dob : birthDate != null )
	then
		ActiveMeasure mam = (activeMeasureBuilder.newActiveMeasureWithAlternateStartDate(175 , $Dob)); insert(mam)
end


rule "AM175_DENOM_EXCL_DX_6082"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 175)
		 java.util.Collection( empty == false ) from collect ( DiagnosticEvent( elements contains 6082 , startDate <= measurementEndDate )) 
	then
		$am.setExcludedFromDenominator(true)
end


rule "AM175_NUM_PROC_4499_RX_9403"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 175)
		$9yearAfterBirth : java.util.Date() from GlobalFunctions.getLaterDate($am.getAlternateStartDate(), 9, DateTimeUnit.YEAR)
		$13yearAfterBirth : java.util.Date() from GlobalFunctions.getLaterDate($am.getAlternateStartDate(), 13, DateTimeUnit.YEAR)
		 $proc : java.util.Collection( empty == false ) from collect ( ProcedureEvent( elements contains 4499 , startDate <= ( $13yearAfterBirth )  && <= measurementEndDate  && >= ( $9yearAfterBirth ) )) 
		 $Rx : java.util.Collection( empty == false ) from collect ( DrugEvent( elements contains 9403 , startDate <= ( $13yearAfterBirth )  && <= measurementEndDate  && >= ( $9yearAfterBirth ) )) 
		totalCollection:java.util.Collection() from collect (java.lang.Object() from (GlobalFunctions.collectAll($proc, $Rx)))
		java.util.Collection(size >=  3) from collect (Event() from(GlobalFunctions.removeTooCloseEvents(totalCollection, 1, DateTimeUnit.DAY)))
	then
		$am.setInNumerator(true)
end


