rule "AM188_DENOM"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18 )
		 Classifier( id == 170 )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(188); insert(mam)
end


rule "AM188_DENOM_EXCLUSION_DX_2315"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 188 )
		 java.util.Collection( empty == false ) from collect ( DiagnosticEvent( elements contains 2315 , endDate <= measurementEndDate )) 
	then
		$am.setExcludedFromDenominator(true)
end


rule "AM188_DENOM_EXCLUSION_DX_6200"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 188 )
		 java.util.Collection( empty == false ) from collect ( DiagnosticEvent( elements contains 6200 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate )) 
	then
		$am.setExcludedFromDenominator(true)
end


rule "AM188_NUM_PROC_6584"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 188 )
		 ProcedureEvent( elements contains 6584 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate )
	then
		$am.setInNumerator(true)
end


rule "AM188_NUM_RX_1849"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 188 )
		 DrugEvent( elements contains 1849 , startDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate )
	then
		$am.setInNumerator(true)
end


