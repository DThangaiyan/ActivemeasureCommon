rule "AM231_NUM_HEDIS_COLONOSCOPY"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 231 )
		ProcedureEvent( elements contains 5951 , endDate <= measurementEndDate , endDate >= years10BeforeMeasurementEndDate )
	then
		$am.setInNumerator( true );
end


rule "AM231_DENOM_COLORECTAL_CANCER_SCREENING"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate <= 75  && >= 50 )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(231); insert(mam)
end


