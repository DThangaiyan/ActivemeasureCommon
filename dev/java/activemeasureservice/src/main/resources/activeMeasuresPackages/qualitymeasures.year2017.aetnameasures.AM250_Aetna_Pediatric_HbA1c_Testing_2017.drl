rule "AM250_DENOM_DIABETES_PEDIATRIC_HBA1C_TESTING"
	dialect "mvel"
	when
		 $am : ActiveMeasuresMemberInfo( ageAtMeasurementEndDate <= 18  && >= 6 )
		 Classifier( id in ( 27, 25, 26 ) )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(250); insert(mam)
end


rule "AM250_NUM_LOINC_PEDIATRIC_HBA1C_TESTING"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 250 )
		 LabEvent( elements contains 5887 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate )
	then
		$am.setInNumerator(true)
end


rule "AM250_NUM_PEDIATRIC_HBA1C_TESTING"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 250 )
		 ProcedureEvent( elements contains 5886 , endDate >= months12BeforeMeasurementEndDate  && <= measurementEndDate )
	then
		$am.setInNumerator(true)
end


