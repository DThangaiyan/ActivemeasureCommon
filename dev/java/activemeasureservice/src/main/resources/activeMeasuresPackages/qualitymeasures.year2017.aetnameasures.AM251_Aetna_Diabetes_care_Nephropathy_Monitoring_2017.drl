rule "AM251_DENOM_AETNA_NEPHROPATHY_MONITORING"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18 , ageAtMeasurementEndDate <= 75 )
		 Classifier( id in ( 27, 25, 26 ) )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(251); insert(mam)
end


rule "AM251_DENOM_EXCL_AETNA_NEPHROPATHY_MONITORING"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 251 )
		 not (DiagnosticEvent( elements contains 5893 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		 not (ProcedureEvent( elements contains 5891 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		 not (ProcedureEvent( elements contains 8786 , careProviderSpecialtyGroupCode == "SPECIALTYGRP_NEPHRO" , endDate >= months12BeforeMeasurementEndDate , endDate <= measurementEndDate )) 
		 not (LabEvent( elements contains 5892 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		 not (DrugEvent( elements contains 1899 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		 not (DrugEvent( endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
	then
		$am.setExcludedFromDenominator(true)
end


