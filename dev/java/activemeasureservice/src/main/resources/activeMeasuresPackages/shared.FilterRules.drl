rule "COMMON_HIE_MOOD"
	salience 1000
	dialect "mvel"
	when
		$event : Event( HIE == true , mood != null  && != HieMood.EVENT  && != HieMood.INTENT )
	then
		retract( $event );
end


rule "COMMON_HIE_NEGATION_FLAG"
	salience 1000
	dialect "mvel"
	when
		$event : Event( HIE == true , negated == true )
	then
		retract( $event );
end




