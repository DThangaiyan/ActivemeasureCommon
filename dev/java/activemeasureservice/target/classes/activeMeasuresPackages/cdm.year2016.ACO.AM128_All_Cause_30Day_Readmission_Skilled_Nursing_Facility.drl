rule "AM128_DENOM_ADMISSION_SNF_WITHIN_1DAY"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 65 )
		 $admittingEventInHospital : ClaimHeader( $admittingEventDischargeDate : endDate <= measurementEndDate , inpatient == true , claimType == ClaimType.INPAT  || == ClaimType.INPAT_FULLENC , endDate >= months12BeforeMeasurementEndDate , dischargeDisposition == DischargeDispositionStatus.CD_03 , startDate != null )
		$1DayAfterAdmittingEventDischargeDate : java.util.Date() from GlobalFunctions.getLaterDate($admittingEventDischargeDate, 1, DateTimeUnit.DAY)
		 $snfAdmittingEvent : ClaimHeader( this != $admittingEventInHospital , endDate <= measurementEndDate , startDate >= $admittingEventDischargeDate , startDate < ( $1DayAfterAdmittingEventDischargeDate ) , claimType == ClaimType.SNF_NONSWING  || == ClaimType.SNF_SWING )
	then
		FactLevelMeasureDenominator flmDenominator= factLevelMeasureBuilder.newFactLevelMeasureDenominatorWithPrimaryFact(128, $admittingEventInHospital); insert(flmDenominator)
end


rule "AM128_NUM_30DAY_READMN_WITH_CDM_ACUTE_DIAGNOSIS"
	dialect "mvel"
	when
		 $flmDenom : FactLevelMeasureDenominator( measureId == 128 , $hospitalAdmission : primaryOriginationFact != null )
		 ClaimHeader( $dischargeDate : endDate != null ) from $hospitalAdmission
		$30DaysAfterHospitalDischargeDate : java.util.Date() from GlobalFunctions.getLaterDate($dischargeDate, 30, DateTimeUnit.DAY)
		 $numPrimaryFact : ClaimHeader( allDiagnosticEventElements contains 7940 , startDate >= $dischargeDate , this != $hospitalAdmission , startDate <= ( $30DaysAfterHospitalDischargeDate ) , inpatient == true , admissionSourceCode == "5" , claimType == ClaimType.INPAT  || == ClaimType.INPAT_FULLENC )
	then
		FactLevelMeasureNumerator flmNumerator = $flmDenom.createFactLevelMeasureNumeratorWithPrimaryFact($numPrimaryFact); insert(flmNumerator);
end


rule "AM128_NUM_EXCL_OTHER_POTENTIAL_PROC_AND_NOT_PRINCIPAL_DIAG_CDM_ACUTE"
	dialect "mvel"
	when
		$flmNum : FactLevelMeasureNumerator( measureId == 128 , $numPrimaryFact : primaryFact != null )
		ClaimHeader( principalDiagnosisElements excludes 7940 , relatedProcedureEventElements contains 8831 ) from $numPrimaryFact
	then
		$flmNum.setExcludedFromNumerator( true );
end


