rule "COMMON_LABEL_ACO_CAD_ENCOUNTER_1_OVERLAP_PAST_12MO"
	salience 100
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18 )
		 primaryEvent : java.util.Collection( ) from collect ( ProcedureEvent( endDate <= measurementEndDate , elements contains 7758 , endDate >= months12BeforeMeasurementEndDate )) 
		 secondaryEvent1 : java.util.Collection( ) from collect ( DiagnosticEvent( endDate <= measurementEndDate , elements contains 6242 , endDate >= months12BeforeMeasurementEndDate )) 
		 secondaryEvent2 : java.util.Collection( ) from collect ( ProcedureEvent( endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate , elements contains 6220 )) 
net.ahm.careengine.domain.measures.active.ActiveMeasureRuleContext(); 		overlapname : java.util.Collection(size >=  1) from collect (OverlappedEvents() from(GlobalFunctions.getOverlappedEvents(primaryEvent, 1, DateTimeUnit.DAY, true, 0,DateTimeUnit.DAY, GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 12, DateTimeUnit.MONTH), measurementEndDate, secondaryEvent1,secondaryEvent2)))
	then
		Classifier classifier = classifierBuilder.getClassifierForQualityMeasure(17); insert(classifier)
end


