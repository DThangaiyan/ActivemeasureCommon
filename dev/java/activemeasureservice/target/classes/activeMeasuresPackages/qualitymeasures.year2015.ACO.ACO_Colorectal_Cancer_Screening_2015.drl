rule "DENOM_ACO_COLORECTAL_SCREENING_2015"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 51 , ageAtMeasurementEndDate <= 75 , gender != null )
		 ProcedureEvent( elements contains 8684 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(13); insert(mam)
end


rule "DENOM_EXCL_ACO_COLORECTAL_CANCER_2015"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 13)
		 DiagnosticEvent( elements contains 8022 , endDate <= measurementEndDate )
	then
		$am.setExcludedFromDenominator(true)
end


