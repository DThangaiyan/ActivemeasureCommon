rule "NUM_ACO_PHQ_LOINC"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 26 , alternateEndDate != null , alternateStartDate != null )
		 $labEvents : java.util.Collection( empty == false ) from collect ( LabResultEvent( elements contains 8824 , endDate <= $am.alternateEndDate  && >= $am.alternateStartDate , resultValue < 5 )) 
		 $adultEncounters : java.util.Collection( empty == false ) from collect ( ProcedureEvent( elements contains 6204 , endDate <= measurementEndDate  && >= months27BeforeMeasurementEndDate )) 
net.ahm.careengine.domain.measures.active.ActiveMeasureRuleContext(); 		overlapname : java.util.Collection(size >=  1) from collect (OverlappedEvents() from(GlobalFunctions.getOverlappedEvents($labEvents, 30, DateTimeUnit.DAY, true, 1,DateTimeUnit.DAY, GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 26, DateTimeUnit.MONTH), measurementEndDate, $adultEncounters)))
	then
		 $am.setInNumerator( true );
end


