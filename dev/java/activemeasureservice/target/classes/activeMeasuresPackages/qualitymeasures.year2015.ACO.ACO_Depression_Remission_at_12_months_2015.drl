rule "DENOM_DEPRESSION_DIAGNOSIS"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18 , $dob : birthDate != null , gender != null )
		$18thBirthDay : java.util.Date() from GlobalFunctions.getLaterDate($dob, 18, DateTimeUnit.YEAR)
		 $labEvent : LabResultEvent( elements contains 8824 , resultValue > 9 , $indexDate : endDate <= months13BeforeMeasurementEndDate , endDate >= months25BeforeMeasurementEndDate , endDate >= ( $18thBirthDay ) )
		 not (LabResultEvent( elements contains 8824 , resultValue > 9 , endDate <= months13BeforeMeasurementEndDate , endDate < $labEvent.endDate , endDate >= months25BeforeMeasurementEndDate , endDate >= ( $18thBirthDay ) , this != $labEvent )) 
		 $labEventCollection : java.util.Collection() from collect( LabResultEvent() from ($labEvent) )
		 $AdultEncounters : java.util.Collection( empty == false ) from collect ( ProcedureEvent( elements contains 6204 , endDate >= months27BeforeMeasurementEndDate  && <= measurementEndDate )) 
net.ahm.careengine.domain.measures.active.ActiveMeasureRuleContext(); 		overlapname : java.util.Collection(size >=  1) from collect (OverlappedEvents() from(GlobalFunctions.getOverlappedEvents($labEventCollection, 30, DateTimeUnit.DAY, true, 1,DateTimeUnit.DAY, GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 25, DateTimeUnit.MONTH), measurementEndDate, $AdultEncounters)))
		 java.util.Collection( empty == false ) from collect ( DiagnosticEvent( endDate >= months12BeforeMeasurementEndDate  && <= measurementEndDate , elements contains 8822  || contains 8823 )) 
		$altStartDate : java.util.Date() from GlobalFunctions.getLaterDate($indexDate, 11, DateTimeUnit.MONTH)
		$altEndDate : java.util.Date() from GlobalFunctions.getLaterDate($indexDate, 13, DateTimeUnit.MONTH)
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasureWithAlternateStartAndEndDate(26 , $altStartDate , $altEndDate); insert(mam)
end


rule "DENOM_EXCL_BIPOLAR_OR_PERSONALITY_DISORDER"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 26 )
		DiagnosticEvent( endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate , elements contains 3035  || contains 8826 )
	then
		$am.setExcludedFromDenominator( true );
end


