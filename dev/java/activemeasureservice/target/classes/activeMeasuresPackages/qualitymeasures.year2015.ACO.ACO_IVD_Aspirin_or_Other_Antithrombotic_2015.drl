global java.util.Date months3BeforeMeasurementEndDate
global java.util.Date months1BeforeMeasurementEndDate














rule "DENOM_LABELS"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( gender != null , ageAtMeasurementEndDate >= 18 )
		 Classifier( id == 12  || == 13 )
		 Classifier( id in ( 15, 16, 14 ) )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(14); insert(mam)
end


rule "DENOM_EXCL_ACO_ASPIRIN_NOT_PRESCRIBED_PROCEDURE"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 14)
		 ProcedureEvent( elements contains 7915 , endDate <= measurementEndDate )
	then
		$am.setExcludedFromDenominator(true)
end


rule "DENOM_EXCL_QM_ASPIRIN_CONTRAINDICATIONS_DIAGNOSTIC"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 14)
		 DiagnosticEvent( elements contains 6247 , endDate <= measurementEndDate )
	then
		$am.setExcludedFromDenominator(true)
end


rule "DENOM_EXCL_ANTICOAGULANTS_ALL_DRUG"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 14)
		 DrugEvent( elements contains 6589 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate , daysSupplyEndDate >= months1BeforeMeasurementEndDate )
	then
		$am.setExcludedFromDenominator(true)
end


rule "NUM_ASPIRIN_OR_ORAL_ANTIPLATELET_DRUG"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 14)
		 DrugEvent( elements contains 2561  || contains 7913 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate )
	then
		$am.setInNumerator(true)
end


rule "NUM_PDD_PATIENT_ON_ASA_ANTIPLATELET"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 14)
		 PatientDerivedEvent( elements contains 7950 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate )
	then
		$am.setInNumerator(true)
end


rule "DENOM_EXCL_PDD_QM_NO_IVD_OR_CONTRAINDICATED"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 14)
		 PatientDerivedEvent( elements contains 8369  || contains 8783 , endDate <= measurementEndDate )
	then
		$am.setExcludedFromDenominator(true)
end




