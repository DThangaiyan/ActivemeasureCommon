rule "DENOM_ANNUAL_MONITORING_PERSISTENT_MEDICATIONS_ACEI_ARB"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18 , gender != null )
		 $drugEventCollection : java.util.Collection( empty == false ) from collect ( DrugEvent( elements contains 1899 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		 $drugEventCollectionPriorYear : java.util.Collection( ) from collect ( DrugEvent( elements contains 1899 , endDate <= measurementEndDate , endDate >= months24BeforeMeasurementEndDate )) 
		net.ahm.careengine.domain.measures.active.ActiveMeasureRuleContext(); java.lang.Number(intValue >=  180) from (GlobalFunctions.getTotalDaysSupplyOfDrugs($drugEventCollectionPriorYear,months24BeforeMeasurementEndDate,measurementEndDate))
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(39); insert(mam)
end


rule "NUM_PDD_SERUM_TEST"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 39 )
		PatientDerivedEvent( elements contains 8865 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )
	then
		$am.setInNumerator( true );
end


