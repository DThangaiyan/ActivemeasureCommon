rule "DENOM_ADULT_BMI_ASSESSMENT"
	dialect "mvel"
	when
		$jan1stMsYearPrior : java.util.Date() from GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 2, DateTimeUnit.YEAR)
		$year18BeforeMsYearPrior : java.util.Date() from GlobalFunctions.getEarlierDate($jan1stMsYearPrior, 18, DateTimeUnit.YEAR)
		 ActiveMeasuresMemberInfo( gender != null , ageAtMeasurementEndDate <= 74 , $birthDate : birthDate <= ( $year18BeforeMsYearPrior ) )
		$year19AfterDOB : java.util.Date() from GlobalFunctions.getLaterDate($birthDate, 19, DateTimeUnit.YEAR)
		 ProcedureEvent( endDate <= measurementEndDate , endDate >= months24BeforeMeasurementEndDate , elements contains 6537 )
	then
		ActiveMeasure mam = (activeMeasureBuilder.newActiveMeasureWithAlternateStartDate(36 , $year19AfterDOB)); insert(mam)
end


rule "DENOM_EXCL_HEDIS_PREGNANCY"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 36 )
		ActiveMeasuresMemberInfo( gender == Gender.FEMALE )
		DiagnosticEvent( endDate <= measurementEndDate , endDate >= months24BeforeMeasurementEndDate , elements contains 6200 )
	then
		$am.setExcludedFromDenominator( true );
end


rule "DENOM_EXCL_PDD_PREGNANCY"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 36 )
		PatientDerivedEvent( elements contains 7963 , endDate <= measurementEndDate , endDate >= months24BeforeMeasurementEndDate )
	then
		$am.setExcludedFromDenominator( true );
end




rule "NUM_DIAG_HEDIS_BMI_PERCENTILE_FOR_YOUNGER_THAN_19"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 36 )
		DiagnosticEvent( endDate <= measurementEndDate , endDate >= months24BeforeMeasurementEndDate , elements contains 6193 , endDate < ( $am.alternateStartDate ) )
	then
		$am.setInNumerator( true );
end


rule "NUM_DIAG_HEDIS_BMI_FOR_OLDER_THAN_EQ_19"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 36 )
		DiagnosticEvent( endDate <= measurementEndDate , endDate >= months24BeforeMeasurementEndDate , elements contains 4101 , endDate >= ( $am.alternateStartDate ) )
	then
		$am.setInNumerator( true );
end






