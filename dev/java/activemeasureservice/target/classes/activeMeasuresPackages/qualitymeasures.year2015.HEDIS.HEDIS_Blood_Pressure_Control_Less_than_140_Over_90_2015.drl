rule "DENOM_DIABETES_BLOOD_PRESSURE_CONTROL"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18  && <= 75 )
		 Classifier( id in ( 27, 25, 26 ) )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(42); insert(mam)
end




rule "NUM_DIABETES_BLOOD_PRESSURE_CONTROL"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 42 )
		Classifier( id == 35  || == 34 )
	then
		$am.setInNumerator( true );
end




