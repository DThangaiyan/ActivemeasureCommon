rule "DENOM_EXCL_CONTINUOUS_PHASE_TREATMENT_DRUG_105DAYS_PRIOR_IPSD"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 49 )
		$priorToIPSD : java.util.Date() from GlobalFunctions.startDayOfRangeEndingOn($am.getAlternateStartDate(), 106, DateTimeUnit.DAY)
		 $earliestDrugEvent : DrugEvent( elements contains 1868 , endDate >= ( $priorToIPSD ) , endDate < $am.alternateStartDate )
		 not (DrugEvent( elements contains 1868 , endDate < $earliestDrugEvent.endDate , this != $earliestDrugEvent , endDate >= ( $priorToIPSD ) )) 
	then
		 $am.setExcludedFromDenominator( true );
end


rule "NUM_CONTINUOUS_PHASE_DRUG_231DAYS_AFTER_IPSD"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 49 )
		$after231DaysFromIPSDIncludingIPSD : java.util.Date() from GlobalFunctions.getLaterDate($am.getAlternateStartDate(), 230, DateTimeUnit.DAY)
		 $drugSup : java.util.Collection( ) from collect ( DrugEvent( elements contains 1868 , daysSupplyEndDate >= $am.alternateStartDate , startDate <= ( $after231DaysFromIPSDIncludingIPSD ) )) 
		net.ahm.careengine.domain.measures.active.ActiveMeasureRuleContext(); java.lang.Number(intValue >=  180) from (GlobalFunctions.getTotalDaysSupplyOfDrugs($drugSup,$am.getAlternateStartDate(),$after231DaysFromIPSDIncludingIPSD))
	then
		 $am.setInNumerator( true );
end


