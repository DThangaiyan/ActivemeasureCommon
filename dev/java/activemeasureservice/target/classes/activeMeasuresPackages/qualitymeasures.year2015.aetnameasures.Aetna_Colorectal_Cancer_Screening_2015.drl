rule "DENOM_COLORECTAL_CANCER"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 50  && <= 75 )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(62); insert(mam)
end


rule "DENOM_EXCL_COLECTOMY_PROCEDURE"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 62 )
		ProcedureEvent( elements contains 5945 , endDate <= measurementEndDate )
	then
		$am.setExcludedFromDenominator( true );
end


rule "DENOM_EXCL_COLORECTAL_CANCER_DIAGNOSIS"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 62 )
		DiagnosticEvent( elements contains 5946 , endDate <= measurementEndDate )
	then
		$am.setExcludedFromDenominator( true );
end


rule "NUM_FECAL_OCCULT_BLOOD_TEST_PROC"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 62 )
		ProcedureEvent( elements contains 5948 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )
	then
		$am.setInNumerator( true );
end


