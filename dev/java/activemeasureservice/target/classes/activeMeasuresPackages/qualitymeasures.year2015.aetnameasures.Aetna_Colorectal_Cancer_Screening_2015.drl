rule "NUM_ELIGIBILITY_MULTIPLE_GAPS"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 62 )
		 not (ProcedureEvent( elements contains 5948 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		 not (ProcedureEvent( elements contains 5950 , endDate >= years5BeforeMeasurementEndDate , endDate <= measurementEndDate )) 
		 not (ProcedureEvent( elements contains 5951 , endDate >= years10BeforeMeasurementEndDate , endDate <= measurementEndDate )) 
		 not (LabEvent( elements contains 5949 , endDate >= months12BeforeMeasurementEndDate , endDate <= measurementEndDate )) 
		oneYearBeforeMeasurementEndDate : java.util.Date() from GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 12, DateTimeUnit.MONTH)
		MemberInfo ( $initialGapDays2 : enrolmentGapDays ); java.util.Collection( size >=  2 ) from collect ( ContiguousDays( durationInDays >=  1) from GlobalFunctions.filterWholeDays( $initialGapDays2, GlobalFunctions.startDayOfRangeEndingOn( oneYearBeforeMeasurementEndDate , 1, DateTimeUnit.YEAR ) , oneYearBeforeMeasurementEndDate  ) )
	then
		 $am.setEligible( false );
end




rule "NUM_ELIGIBILITY_46_DAYS_OR_MORE"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 62 )
		 not (ProcedureEvent( elements contains 5948 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		 not (ProcedureEvent( elements contains 5950 , endDate >= years5BeforeMeasurementEndDate , endDate <= measurementEndDate )) 
		 not (ProcedureEvent( elements contains 5951 , endDate >= years10BeforeMeasurementEndDate , endDate <= measurementEndDate )) 
		 not (LabEvent( elements contains 5949 , endDate >= months12BeforeMeasurementEndDate , endDate <= measurementEndDate )) 
		oneYearBeforeMeasurementEndDate : java.util.Date() from GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 12, DateTimeUnit.MONTH)
		MemberInfo ( $initialGapDays2 : enrolmentGapDays ); java.util.Collection( size >=  1 ) from collect ( ContiguousDays( durationInDays >=  46) from GlobalFunctions.filterWholeDays( $initialGapDays2, GlobalFunctions.startDayOfRangeEndingOn( oneYearBeforeMeasurementEndDate , 1, DateTimeUnit.YEAR ) , oneYearBeforeMeasurementEndDate  ) )
	then
		 $am.setEligible( false );
end


