rule "AM138_DENOM_ACO_HEART_FAILURE_BETA_BLOCKER_THERAPHY_FOR_LVSD_LVEF_DIAG"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( gender != null , ageAtMeasurementEndDate >= 18 )
		 java.util.Collection( empty == false ) from collect ( DiagnosticEvent( elements contains 6273 , endDate <= measurementEndDate )) 
		 java.util.Collection( empty == false ) from collect ( ProcedureEvent( elements contains 6110 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate )) 
		 java.util.Collection( empty == false ) from collect ( DiagnosticEvent( elements contains 8707 , endDate <= measurementEndDate )) 
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(138); insert(mam)
end


