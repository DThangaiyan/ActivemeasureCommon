rule "AM138_DENOM_EXCL_CONSECUTIVE_HEART_RATE_LAB_6415"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 138)
		 $labEvent1 : LabResultEvent( elements contains 6415 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate , resultValue < 50 )
		 $labEvent2 : LabResultEvent( elements contains 6415 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate , resultValue < 50 , this != $labEvent1 , endDate > $labEvent1.endDate )
		 not (LabResultEvent( elements contains 6415 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate , endDate > $labEvent1.endDate , endDate < $labEvent2.endDate , this != $labEvent1 , this != $labEvent2 )) 
	then
		$am.setExcludedFromDenominator(true)
end


