rule "AM141_DENOM_EXCL"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 141 )
		( ProcedureEvent( elements contains 2675 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate ) or DiagnosticEvent( elements contains 8300 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate ) or PatientDerivedEvent( elements contains 8370 , endDate <= measurementEndDate ) )
	then
		$am.setExcludedFromDenominator( true );
end


rule "AM141_NUM_ABSENCE_LAB_HBA1C_PROC_CPT"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 141 )
		 not (LabResultEvent( endDate >= months12BeforeMeasurementEndDate , endDate <= measurementEndDate , elements contains 5887 )) 
		 not (ProcedureEvent( elements contains 5886 , endDate >= months12BeforeMeasurementEndDate , endDate <= measurementEndDate )) 
	then
		$am.setInNumerator(true)
end


rule "AM141_NUM_PDD_HBA1C_GR_THAN_9"
