rule "AM141_DENOM_7761_AND_7762_OVERLAP_7753"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate <= 75 , gender != null , ageAtMeasurementEndDate >= 18 )
		 Classifier( id == 20 )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(141); insert(mam)
end


rule "AM141_DENOM_7761_AND_7762_OVERLAP_5864"
	dialect "mvel"
