rule "AM146_DENOM_EXCL"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 146)
		 ( DiagnosticEvent( elements contains 4131 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate ) or ProcedureEvent( elements contains 7797 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate ) or ProcedureEvent( elements contains 2675 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate ) or DiagnosticEvent( elements contains 8300 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate ) )
	then
		$am.setExcludedFromDenominator(true)
end


rule "AM146_NUM_DIAG_VACCINE_INFLUENZA"
	dialect "mvel"
	when
		$am : ActiveMeasure( $fluStartDate : alternateStartDate != null  , measureId == 146 , $fluEndDate : alternateEndDate != null )
		DiagnosticEvent( endDate <= $fluEndDate , endDate >= $fluStartDate , elements contains 2326 )
	then
		$am.setInNumerator( true );
end


rule "AM146_NUM_DRUG_VACCINE_INFLUENZA"
	dialect "mvel"
	when
		$am : ActiveMeasure( $fluStartDate : alternateStartDate != null  , measureId == 146 , $fluEndDate : alternateEndDate != null )
		DrugEvent( elements contains 2755 , endDate >= $fluStartDate , endDate <= $fluEndDate )
	then
		$am.setInNumerator( true );
end


rule "AM146_NUM_PROC_VACCINE_INFLUENZA"
	dialect "mvel"
	when
		$am : ActiveMeasure( $fluStartDate : alternateStartDate != null  , measureId == 146 , $fluEndDate : alternateEndDate != null )
		ProcedureEvent( endDate <= $fluEndDate , endDate >= $fluStartDate , elements contains 2105 )
	then
		$am.setInNumerator( true );
end


rule "AM146_DENOM_8787_FROM_HIE"
	dialect "mvel"
	when
		$Oct1PriorYr : java.util.Date() from CEDateUtil.getEalierDateForYear(measurementEndDate, 1, 1, Month.OCTOBER)
		$month6Old : java.util.Date() from GlobalFunctions.getEarlierDate($Oct1PriorYr, 6, DateTimeUnit.MONTH)
		 ActiveMeasuresMemberInfo( $dateOfBirth : birthDate != null  && <= ( $month6Old ) , gender != null )
		$dob6Mo : java.util.Date() from GlobalFunctions.getLaterDate($dateOfBirth, 6, DateTimeUnit.MONTH)
		$fluStartDate : java.util.Date() from CEDateUtil.getEalierDateForYear(measurementEndDate, 1, 1, Month.OCTOBER)
		$fluEndDate : java.util.Date() from CEDateUtil.getEalierDateForYear(measurementEndDate, 0, 31, Month.MARCH)
		$NumFluStartDate : java.util.Date() from CEDateUtil.getEalierDateForYear(measurementEndDate, 1, 1, Month.AUGUST)
		 ProcedureEvent( elements contains 8787 , endDate >= ( $dob6Mo ) , endDate >= ( $fluStartDate ) , endDate <= ( $fluEndDate )  && <= ( measurementEndDate ) , HIE == true )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasureWithAlternateStartAndEndDate(146 , $NumFluStartDate , $fluEndDate); insert(mam)
end


rule "AM146_DENOM_6204_FROM_CLAIM"
	dialect "mvel"
	when
		$Oct1PriorYr : java.util.Date() from CEDateUtil.getEalierDateForYear(measurementEndDate, 1, 1, Month.OCTOBER)
		$month6Old : java.util.Date() from GlobalFunctions.getEarlierDate($Oct1PriorYr, 6, DateTimeUnit.MONTH)
		 ActiveMeasuresMemberInfo( $dateOfBirth : birthDate != null  && <= ( $month6Old ) , gender != null )
		$dob6Mo : java.util.Date() from GlobalFunctions.getLaterDate($dateOfBirth, 6, DateTimeUnit.MONTH)
		$fluStartDate : java.util.Date() from CEDateUtil.getEalierDateForYear(measurementEndDate, 1, 1, Month.OCTOBER)
		$fluEndDate : java.util.Date() from CEDateUtil.getEalierDateForYear(measurementEndDate, 0, 31, Month.MARCH)
		$NumFluStartDate : java.util.Date() from CEDateUtil.getEalierDateForYear(measurementEndDate, 1, 1, Month.AUGUST)
		 java.util.Collection( size >= 2 ) from collect ( ProcedureEvent( elements contains 6204 , endDate >= ( $dob6Mo ) , endDate >= ( $fluStartDate ) , endDate <= ( $fluEndDate )  && <= ( measurementEndDate ) , claim == true )) 
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasureWithAlternateStartAndEndDate(146 , $NumFluStartDate , $fluEndDate); insert(mam)
end




