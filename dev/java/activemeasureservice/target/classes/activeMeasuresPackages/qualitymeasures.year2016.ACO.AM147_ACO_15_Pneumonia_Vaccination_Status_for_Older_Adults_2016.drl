global java.util.Date measurementEndDate
global net.ahm.careengine.domain.measures.active.ActiveMeasureBuilder activeMeasureBuilder
global java.util.Date months12BeforeMeasurementEndDate









rule "AM147_DENOM_ACO_QM_ENCOUNTER_ADULT"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( gender != null , ageAtMeasurementEndDate >= 65 )
		 ProcedureEvent( endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate , elements contains 6204 )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(147); insert(mam)
end


rule "AM147_DENOM_EXCL"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 147)
		 ( ProcedureEvent( elements contains 2675 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate ) or DiagnosticEvent( elements contains 8300 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate ) )
	then
		$am.setExcludedFromDenominator(true)
end


rule "AM147_NUM_VACCINE_PNEUMOCOCCAL_23_VALENT_DRUG"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 147)
		 DrugEvent( elements contains 2753 , endDate <= measurementEndDate )
	then
		$am.setInNumerator(true)
end


rule "AM147_NUM_VACCINE_PNEUMOCOCCAL_23_VALENT_PROC"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 147)
		 ProcedureEvent( endDate <= measurementEndDate , elements contains 1739 )
	then
		$am.setInNumerator(true)
end


rule "AM147_NUM_VACCINE_PNEUMOCOCCAL_DX_DIAGNOSIS"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 147)
		 DiagnosticEvent( endDate <= measurementEndDate , elements contains 8708 )
	then
		$am.setInNumerator(true)
end




