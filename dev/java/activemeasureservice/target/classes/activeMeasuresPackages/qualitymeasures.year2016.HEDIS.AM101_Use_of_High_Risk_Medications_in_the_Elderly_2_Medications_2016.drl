global java.util.Date months24BeforeMeasurementEndDate
global java.util.Date months27BeforeMeasurementEndDate
global java.util.Date years10BeforeMeasurementEndDate
global java.util.Date years5BeforeMeasurementEndDate































rule "AM101_DENOM"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( gender != null , ageAtMeasurementEndDate >= 66 )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(101); insert(mam)
end


rule "AM101_NUM_DRUG_9013_AND_OTHER"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 101 )
		 DrugEvent( elements contains 9013 , endDate >= months12BeforeMeasurementEndDate  && <= measurementEndDate )
		 Classifier( id in ( 112, 111, 113 ) )
	then
		$am.setInNumerator(true)
end


rule "AM101_NUM_TWO_DRUG"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 101 )
		 Classifier( id in ( 112, 113 ) )
		 Classifier( id == 111 )
	then
		$am.setInNumerator(true)
end




