rule "AM117_DENOM"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 2  && < 3 , $Dob : birthDate != null , gender != null )
		$year2AfterDob : java.util.Date() from GlobalFunctions.getLaterDate($Dob, 2, DateTimeUnit.YEAR)
		$42daysAfterBirth : java.util.Date() from GlobalFunctions.getLaterDate($Dob, 42, DateTimeUnit.DAY)
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasureWithAlternateStartAndEndDate(117 , $year2AfterDob , $42daysAfterBirth); insert(mam)
end


rule "AM117_DENOM_EXCL_DX_6082"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 117 )
		DiagnosticEvent( elements contains 6082 , startDate <= measurementEndDate )
	then
		$am.setExcludedFromDenominator( true );
end


rule "AM117_DENOM_EXCL_PDD_8448"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 117 )
		PatientDerivedEvent( elements contains 8448 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )
	then
		$am.setExcludedFromDenominator( true );
end


rule "AM117_NUM_PDD_8447"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 117 )
		 PatientDerivedEvent( elements contains 8447 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )
	then
		$am.setInNumerator(true)
end


rule "AM117_NUM_3_PROC_6089"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 117)
		 collections : java.util.Collection( empty == false ) from collect ( ProcedureEvent( elements contains 6089 , startDate <= ( $am.getAlternateStartDate() )  && <= measurementEndDate , startDate >= ( $am.getAlternateEndDate() ) )) 
		java.util.Collection(size >=  3) from collect (Event() from(GlobalFunctions.removeTooCloseEvents(collections, 1, DateTimeUnit.DAY)))
	then
		$am.setInNumerator(true)
end


