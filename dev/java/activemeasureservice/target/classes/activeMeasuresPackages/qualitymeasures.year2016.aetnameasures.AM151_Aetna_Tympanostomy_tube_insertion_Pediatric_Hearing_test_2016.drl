rule "AM151_DENOM_1_TUBE_INSERTION"
	dialect "mvel"
	when
		$beginingOfMSyear : java.util.Date() from GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 1, DateTimeUnit.YEAR)
		$year2 : java.util.Date() from GlobalFunctions.getEarlierDate($beginingOfMSyear, 2, DateTimeUnit.YEAR)
		$year12 : java.util.Date() from GlobalFunctions.getEarlierDate(measurementEndDate, 12, DateTimeUnit.YEAR)
		 ActiveMeasuresMemberInfo( birthDate <= ( $year2 ) , birthDate >= ( $year12 ) )
		 primaryEvent : java.util.Collection( size >= 1 ) from collect ( DiagnosticEvent( elements contains 9100 , endDate >= months12BeforeMeasurementEndDate , endDate <= measurementEndDate , claim == true )) 
		 secondaryEvent : java.util.Collection( size >= 1 ) from collect ( ProcedureEvent( elements contains 9099 , endDate >= months12BeforeMeasurementEndDate , endDate <= measurementEndDate , claim == true )) 
net.ahm.careengine.domain.measures.active.ActiveMeasureRuleContext(); 		earliestoverlap : OverlappedEvents(this != null) from(GlobalFunctions.getEarliestOverlappedEvent(primaryEvent, 0, DateTimeUnit.DAY, true, 1,DateTimeUnit.DAY, GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 1, DateTimeUnit.YEAR), measurementEndDate, secondaryEvent))
		 java.util.Collection( size == 1 ) from collect ( ProcedureEvent( elements contains 9101 , claim == true , endDate > ( earliestoverlap.getContiguousDays().getStart() ) , endDate <= measurementEndDate )) 
		 ProcedureEvent( elements contains 9101 , claim == true , $indexDate : endDate > ( earliestoverlap.getContiguousDays().getStart() ) , endDate <= measurementEndDate )
	then
		ActiveMeasure mam = (activeMeasureBuilder.newActiveMeasureWithAlternateStartDate(151 , $indexDate)); insert(mam)
end


rule "AM151_DENOM_2_OR_MORE_TUBE_INSERTION"
	dialect "mvel"
	when
		$Jan1stMsYr : java.util.Date() from GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 12, DateTimeUnit.MONTH)
		$yr2beforeJan1stMsYr : java.util.Date() from GlobalFunctions.getEarlierDate($Jan1stMsYr, 2, DateTimeUnit.YEAR)
		$yr12 : java.util.Date() from GlobalFunctions.getEarlierDate(measurementEndDate, 12, DateTimeUnit.YEAR)
		 ActiveMeasuresMemberInfo( birthDate <= ( $yr2beforeJan1stMsYr ) , birthDate >= ( $yr12 ) )
		 primaryEvent : java.util.Collection( size >= 1 ) from collect ( DiagnosticEvent( elements contains 9100 , endDate >= months12BeforeMeasurementEndDate , endDate <= measurementEndDate , claim == true )) 
		 secondaryEvent : java.util.Collection( size >= 1 ) from collect ( ProcedureEvent( elements contains 9099 , endDate >= months12BeforeMeasurementEndDate , endDate <= measurementEndDate , claim == true )) 
net.ahm.careengine.domain.measures.active.ActiveMeasureRuleContext(); 		earliestoverlap : OverlappedEvents(this != null) from(GlobalFunctions.getEarliestOverlappedEvent(primaryEvent, 0, DateTimeUnit.DAY, true, 1,DateTimeUnit.DAY, GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 1, DateTimeUnit.YEAR), measurementEndDate, secondaryEvent))
		 java.util.Collection( size >= 2 ) from collect ( ProcedureEvent( elements contains 9101 , claim == true , endDate > ( earliestoverlap.getContiguousDays().getStart() ) , endDate <= measurementEndDate )) 
		 $earliestProc : ProcedureEvent( elements contains 9101 , claim == true , endDate > ( earliestoverlap.getContiguousDays().getStart() ) , endDate <= measurementEndDate )
		 ProcedureEvent( this != $earliestProc , elements contains 9101 , claim == true , endDate > ( $earliestProc.endDate ) , endDate <= measurementEndDate )
	then
		ActiveMeasure mam = (activeMeasureBuilder.newActiveMeasureWithAlternateStartDate(151 , $earliestProc.endDate)); insert(mam)
end




