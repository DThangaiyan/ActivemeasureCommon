rule "AM151_CONTINUOUS_ENROLLMENT"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 151 )
		$month6BeforeIndexDate : java.util.Date() from GlobalFunctions.getEarlierDate($am.getAlternateStartDate(), 6, DateTimeUnit.MONTH)
		MemberInfo ( $initialGapDays2 : enrolmentGapDays ); java.util.Collection( size >=  1 ) from collect ( ContiguousDays( durationInDays >=  1) from GlobalFunctions.filterWholeDays( $initialGapDays2, $month6BeforeIndexDate, $am.getAlternateStartDate() ) )
	then
		$am.setEligible(false)
end


rule "AM151_DENOM_EXCL"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 151 )
		$month6BeforeIndexDate : java.util.Date() from GlobalFunctions.getEarlierDate($am.getAlternateStartDate(), 6, DateTimeUnit.MONTH)
		$July1stPriorYr : java.util.Date() from GlobalFunctions.startDayOfRangeEndingOn(measurementEndDate, 18, DateTimeUnit.MONTH)
		 ProcedureEvent( elements contains 9101 , endDate >= ( $month6BeforeIndexDate ) , endDate >= ( $July1stPriorYr ) , endDate < ( $am.getAlternateStartDate() )  && <= ( measurementEndDate ) , claim == true )
