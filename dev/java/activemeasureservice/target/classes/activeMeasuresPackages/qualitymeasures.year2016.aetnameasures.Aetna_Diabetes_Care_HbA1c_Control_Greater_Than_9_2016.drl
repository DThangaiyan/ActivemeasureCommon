rule "DENOM_DIABETES_HBA1C_POOR_CONTROL"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18 , ageAtMeasurementEndDate <= 75 )
		 Classifier( id in ( 27, 25, 26 ) )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(60); insert(mam)
end


rule "DENOM_EXCL_HEDIS_HBA1C_LT_2_OR_GT_18"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 60 )
		 LabResultEvent( elements contains 5887 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate , resultValue < 2  || > 18 )
	then
		$am.setExcludedFromDenominator(true)
end


rule "NUM_HEDIS_HBA1C_PROC_GT_9"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 60 )
		 Classifier( id == 74 )
	then
		$am.setInNumerator(true)
end


rule "DENOM_EXCL_ABSENCE_HEDIS_HBA1C_PROC_LAB"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 60 )
		 not (ProcedureEvent( elements contains 5886 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		 not (LabEvent( elements contains 5887 , endDate >= months12BeforeMeasurementEndDate , endDate <= measurementEndDate )) 
	then
		$am.setExcludedFromDenominator(true)
end






rule "NUM_HEDIS_HBA1C_LAB_GT_9"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 60 )
		 $latestEvent : CodifiedEvent( elements contains 5887 ) from accumulate ( $allEvents : CodifiedEvent( endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate , elements contains 5887  || contains 5886 ),
			latestEvent($allEvents)) 
		 LabResultEvent( resultValue > 9 , resultValue <= 18 ) from $latestEvent
	then
		$am.setInNumerator(true)
end




