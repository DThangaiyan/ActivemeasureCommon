rule "DENOM_AETNA_NEPHROPATHY_MONITORING"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18 , ageAtMeasurementEndDate <= 75 )
		 java.util.Collection( empty == false ) from collect ( Classifier( id in ( 27, 25, 26 ) )) 
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(58); insert(mam)
end




rule "NUM_HEDIS_NEPHROPATHY_SCREENING_LOINC"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 58 )
		 java.util.Collection( empty == false ) from collect ( LabEvent( elements contains 5892 , endDate >= months12BeforeMeasurementEndDate  && <= measurementEndDate )) 
	then
		$am.setInNumerator(true)
end


rule "NUM_HEDIS_OFFICE_ED_NONACUTE_INPT_VISIT_PROC"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 58 )
		 java.util.Collection( empty == false ) from collect ( ProcedureEvent( elements contains 8786 , endDate >= months12BeforeMeasurementEndDate  && <= measurementEndDate , careProviderSpecialtyGroupCode == "SPECIALTYGRP_NEPHRO" )) 
	then
		$am.setInNumerator(true)
end


rule "NUM_ANTI_HYPE_ARB_ACEI"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 58 )
		 java.util.Collection( empty == false ) from collect ( DrugEvent( elements contains 1899 , endDate >= months12BeforeMeasurementEndDate  && <= measurementEndDate )) 
	then
		$am.setInNumerator(true)
end


rule "NUM_HEDIS_NEPHROPATHY_SCREENING_OR_TREATMENT_CPT"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 58 )
		 java.util.Collection( empty == false ) from collect ( ProcedureEvent( elements contains 5891 , endDate >= months12BeforeMeasurementEndDate  && <= measurementEndDate )) 
	then
		$am.setInNumerator(true)
end


rule "NUM_HEDIS_NEPHROPATHY_SCREENING_OR_TREATMENT_ICD9"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 58 )
		 java.util.Collection( empty == false ) from collect ( DiagnosticEvent( elements contains 5893 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate )) 
	then
		$am.setInNumerator(true)
end


rule "DENOM_EXCL_AETNA_NEPHROPATHY_MONITORING"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 58 )
		 not (DiagnosticEvent( elements contains 5893 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		 not (ProcedureEvent( elements contains 5891  || contains 8786 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		 not (LabEvent( elements contains 5892 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		 not (DrugEvent( elements contains 1899 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		 not (DrugEvent( endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
	then
		$am.setExcludedFromDenominator(true)
end






