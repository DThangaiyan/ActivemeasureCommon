rule "AM171_NUM"
	dialect "mvel"
	when
		$am : ActiveMeasure(measureId == 171)
		 ProcedureEvent( elements contains 9369 , $testDate : startDate >= months12BeforeMeasurementEndDate , startDate <= measurementEndDate )
		 not (DiagnosticEvent( elements contains 9368 , startDate <= measurementEndDate )) 
		$day30AfterTest : java.util.Date() from GlobalFunctions.getLaterDate($testDate, 30, DateTimeUnit.DAY)
		 not (DiagnosticEvent( elements contains 9002 , startDate <= ( $day30AfterTest ) , startDate <= measurementEndDate , startDate >= months12BeforeMeasurementEndDate )) 
		 not (ProcedureEvent( elements contains 9003 , startDate <= ( $day30AfterTest ) , startDate <= measurementEndDate , startDate >= months12BeforeMeasurementEndDate )) 
	then
		$am.setInNumerator(true)
end


rule "AM171_DENOM"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18 )
		 java.util.Collection( empty == false ) from collect ( ProcedureEvent( elements contains 9386 , startDate <= measurementEndDate , startDate >= months12BeforeMeasurementEndDate )) 
		 java.util.Collection( empty == false ) from collect ( DiagnosticEvent( elements contains 9367 , startDate <= measurementEndDate , startDate >= months24BeforeMeasurementEndDate )) 
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(171); insert(mam)
end


