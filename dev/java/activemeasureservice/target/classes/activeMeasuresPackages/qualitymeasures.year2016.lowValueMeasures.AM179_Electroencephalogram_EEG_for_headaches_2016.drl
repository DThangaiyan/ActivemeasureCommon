rule "AM179_DENOM"
	salience 100
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18 )
		 java.util.Collection( empty == false ) from collect ( ProcedureEvent( elements contains 9339 , startDate <= measurementEndDate , startDate >= months12BeforeMeasurementEndDate )) 
		 java.util.Collection( empty == false ) from collect ( DiagnosticEvent( elements contains 9364 , startDate <= measurementEndDate , startDate >= months12BeforeMeasurementEndDate )) 
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(179); insert(mam)
end


rule "AM179_NUM"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 179 )
		 ProcedureEvent( startDate <= measurementEndDate , startDate >= months12BeforeMeasurementEndDate , elements contains 4718 )
		 not (DiagnosticEvent( elements contains 2973 , startDate <= measurementEndDate )) 
	then
		$am.setInNumerator(true)
end


