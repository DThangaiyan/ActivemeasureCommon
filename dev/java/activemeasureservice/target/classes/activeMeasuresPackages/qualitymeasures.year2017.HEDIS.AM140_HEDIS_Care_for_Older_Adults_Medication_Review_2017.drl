rule "AM140_DENOM"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 66 )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(140); insert(mam)
end


rule "AM140_NUM_PROC_6547_6549"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 140 )
		 $proc1 : java.util.Collection( size >= 1 ) from collect ( ProcedureEvent( elements contains 6547 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate )) 
		 $proc2 : java.util.Collection( size >= 1 ) from collect ( ProcedureEvent( elements contains 6549 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate )) 
net.ahm.careengine.domain.measures.active.ActiveMeasureRuleContext(); 		overlap : java.util.Collection(size >=  1) from collect (OverlappedEvents() from(GlobalFunctions.getOverlappedEventsAnytimeInPast($proc1, 0, DateTimeUnit.DAY, true, 1,DateTimeUnit.DAY, measurementEndDate, $proc2)))
	then
		$am.setInNumerator(true)
end


rule "AM140_NUM_PROC_9485"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 140 )
		Jan30 : java.util.Date() from GlobalFunctions.getLaterDate(months12BeforeMeasurementEndDate, 29, DateTimeUnit.DAY)
		Jan22NextYear : java.util.Date() from GlobalFunctions.getLaterDate(measurementEndDate, 22, DateTimeUnit.DAY)
		 $collection : java.util.Collection( size >= 1 ) from collect ( ProcedureEvent( elements contains 9485 , startDate <= ( Jan22NextYear )  && >= ( Jan30 ) )) 
	then
		$am.setInNumerator(true)
end


