rule "AM217_DENOM_ANNUAL_MONITORING_PERSISTENT_MEDICATIONS_DIGOXIN"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18 , gender != null )
		 $drugEventCollection : java.util.Collection( empty == false ) from collect ( DrugEvent( elements contains 365 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		 $drugEventCollectionPriorYear : java.util.Collection( empty == false ) from collect ( DrugEvent( elements contains 365 , endDate <= measurementEndDate , endDate >= months24BeforeMeasurementEndDate )) 
		java.lang.Number(intValue >=  180) from (GlobalFunctions.getTotalDaysSupplyOfDrugs($drugEventCollectionPriorYear,months24BeforeMeasurementEndDate,measurementEndDate))
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(217); insert(mam)
end


rule "AM217_NUM_LAB_PANEL_AND_SERUM_DIGOXIN_TEST"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 217 )
		 java.util.Collection( ) from collect ( ProcedureEvent( elements contains 5889 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
		 ( ProcedureEvent( elements contains 8875 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate ) or LabEvent( elements contains 8874 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate ) )
	then
		$am.setInNumerator(true)
end


rule "AM217_NUM_POTASSIUM_AND_CREATININE_AND_DIGOXIN"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 217 )
		 ( ProcedureEvent( elements contains 8875 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate ) or LabEvent( elements contains 8874 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate ) )
		 ( ProcedureEvent( elements contains 5895 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate ) or LabEvent( elements contains 5896 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate ) )
		 ( ProcedureEvent( elements contains 5890 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate ) or LabEvent( elements contains 5894 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate ) )
	then
		$am.setInNumerator(true)
end


rule "AM217_DENOM_EXCL_HEDIS_HOSPITALIZATION_ACUTE_NONACUTE"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 217 )
		ProcedureEvent( elements contains 5902 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate )
	then
		$am.setExcludedFromDenominator( true );
end


