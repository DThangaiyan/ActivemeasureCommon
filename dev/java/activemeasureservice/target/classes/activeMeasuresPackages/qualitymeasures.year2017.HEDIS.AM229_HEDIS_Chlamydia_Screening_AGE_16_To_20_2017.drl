rule "AM229_DENOM"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( gender == Gender.FEMALE , ageAtMeasurementEndDate <= 20 , ageAtMeasurementEndDate >= 16 )
		 ( DiagnosticEvent( elements contains 6200 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate ) or DrugEvent( elements contains 5973 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate ) or DiagnosticEvent( elements contains 5958 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate ) or ProcedureEvent( elements contains 5959 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate ) )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(229); insert(mam)
end


rule "AM229_DENOM_EXCLUSION"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 229 )
		Classifier( id == 61  || == 62  || == 63  || == 64 )
	then
		$am.setExcludedFromDenominator( true );
end


rule "AM229_NUM_LAB_5956"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 229 )
		LabEvent( elements contains 5956 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )
	then
		$am.setInNumerator( true );
end


rule "AM229_NUM_PROC_5957"
	dialect "mvel"
	when
		$am : ActiveMeasure( measureId == 229 )
		ProcedureEvent( elements contains 5957 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )
	then
		$am.setInNumerator( true );
end


