rule "AM249_DENOM_WELL_CHILD_VISITS"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 3 , ageAtMeasurementEndDate <= 6 )
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(249); insert(mam)
end


rule "AM249_NUM_DIAG_PCP_WELL_CHILD_VISITS"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 249 )
		 DiagnosticEvent( elements contains 6251 , careProviderSpecialtyGroupCode == "SPECIALTYGRP_PCP" , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )
	then
		$am.setInNumerator(true)
end


