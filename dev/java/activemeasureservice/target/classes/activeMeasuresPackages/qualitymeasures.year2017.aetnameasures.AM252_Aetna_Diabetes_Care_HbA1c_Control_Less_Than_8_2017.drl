rule "AM252_DENOM_LAB_5887"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18 , ageAtMeasurementEndDate <= 75 )
		 Classifier( id in ( 27, 25, 26 ) )
		 java.util.Collection( size >= 0 ) from collect ( LabEvent( elements contains 5887 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate )) 
	then
		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(252); insert(mam)
end


rule "AM252_DENOM_PROC_5886"
	dialect "mvel"
	when
		 ActiveMeasuresMemberInfo( ageAtMeasurementEndDate >= 18 , ageAtMeasurementEndDate <= 75 )
		 Classifier( id in ( 27, 25, 26 ) )
		 java.util.Collection( size >= 0 ) from collect ( ProcedureEvent( elements contains 5886 , endDate <= measurementEndDate  && >= months12BeforeMeasurementEndDate )) 
