		ActiveMeasure mam = activeMeasureBuilder.newActiveMeasure(252); insert(mam)
end


rule "AM252_DENOM_EXCL_LATEST_HBA1C_LOINC_LAB_LESS_THAN_2_OR_GR_THAN_18"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 252 )
		 Classifier( id == 195 )
	then
		$am.setExcludedFromDenominator(true)
end


rule "AM252_DENOM_EXCL_ABSENSE_HEDIS_HBA1C_LAB_AND_PROC"
	dialect "mvel"
	when
		 $am : ActiveMeasure( measureId == 252 )
		 not (LabEvent( elements contains 5887 , endDate >= months12BeforeMeasurementEndDate , endDate <= measurementEndDate )) 
		 not (ProcedureEvent( elements contains 5886 , endDate <= measurementEndDate , endDate >= months12BeforeMeasurementEndDate )) 
	then
		$am.setExcludedFromDenominator(true)
end


